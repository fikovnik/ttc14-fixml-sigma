<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
	<appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%-5level %msg%n" />
		</Console>
	</appenders>
	<loggers>
		<root level="ERROR">
			<appender-ref ref="Console" />
		</root>
	</loggers>
</configuration>